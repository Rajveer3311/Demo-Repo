<apex:page controller="Function" lightningStylesheets="true">
    <apex:form >
        <apex:pageBlock >
            <apex:pageBlockSection title="Account Detail">
                <apex:pageblockTable value="{!accList}" var="a">
                    <apex:commandButton title="Save" value=""/>
                    <apex:column value="{!a.Name}"/>
                    <apex:column value="{!a.Id}"/>
                    
                </apex:pageblockTable>
            </apex:pageBlockSection>
        </apex:pageBlock>
    </apex:form>
</apex:page>